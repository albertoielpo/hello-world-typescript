{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "attach",
            "name": "Attach hello world",
            "skipFiles": ["<node_internals>/**"],
            "port": 9229,
            "restart": true
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch hello world",
            "skipFiles": ["<node_internals>/**"],
            "program": "${workspaceFolder}/dist/app.js"
        }
    ]
}
